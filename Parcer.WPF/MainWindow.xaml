<Window x:Class="Parcer.WPF.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Parcer.WPF"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <StackPanel Orientation="Vertical">
            <StackPanel Orientation="Horizontal">
                    <ComboBox x:Name="cmb_brand" Width="250" Margin="10 0 10 0"></ComboBox>
                    <ComboBox x:Name="cmb_model" Width="250" Margin="0 0 10 0"></ComboBox>
                    <ComboBox x:Name="cmb_generation" Width="250"></ComboBox>
            </StackPanel>
            <Button Height="40" Click="Button_Click" >Enter Site</Button>
        </StackPanel>
    </Grid>
</Window>
